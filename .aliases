alias ..='cd ../'
alias ...='cd ../../'
alias ....='cd ../../../'
alias .....='cd ../../../../'
alias dl='cd ~/Downloads/'
alias sc='cd ~/coursework/'
alias proj='cd ~/proj/'
alias dotf='cd  ~/dotfiles/'
alias screens='open ~/Documents/Screenshots/'
alias ll='ls -la'
alias lsg='ls | grep'
alias sl='ls'
alias la='cat ~/dotfiles/.aliases'
alias cl='clear'
alias md='mkdir'
alias zrc='nvim ~/.zshrc'
alias vrc='nvim ~/.vimrc'
alias trc='nvim ~/.tmux.conf'
alias arc='nvim ~/dotfiles/.aliases'
alias top='htop'
alias :q='exit'
alias f='open .'
alias p='python3'
alias pip='python3 -m pip'
alias v='nvim'
alias t='tmux'
alias gpm='git push -u origin master'
alias gc='git commit -m' # handling gpg signing thru .gitconfig now
alias gcr='git commit --reuse-message=HEAD' # reuse last commit message
alias ga='git add'
alias gs='git status -s'
alias gsl='git status'
alias gd='git diff-index --quiet HEAD -- || clear; git --no-pager diff --patch-with-stat'
alias brewu='brew update && brew upgrade && brew cleanup && brew doctor'
alias afk='/System/Library/CoreServices/Menu\ Extras/User.menu/Contents/Resources/CGSession -suspend'
alias ip='dig +short myip.opendns.com @resolver1.opendns.com'
alias localip='ipconfig getifaddr en0'
alias ws='workstation'
alias td='cd $(mktemp -d /tmp/$1.XXXX)'
alias hsp='hugo server --renderToDisk --disableFastRender'
alias j='java'
alias jc='javac'
alias pls='sudo $(history -p !!)'
alias did="nvim +'normal Go' +'r!date' ~/vimwiki/did.md"
alias ta="tmux a"
alias venv=". venv/bin/activate"
